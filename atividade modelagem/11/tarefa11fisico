/* Lógico_1: */

CREATE TABLE controle (
    id_cliente integer PRIMARY KEY,
    endereco varchar(200),
    telefone varchar(200),
    data_nascimento varchar(200),
    data_primeira_conslta varchar(200),
    email varchar(200),
    tipo_plano varchar(200)
);

CREATE TABLE clientes (
    id_novo_cliente integer PRIMARY KEY,
    telefone varchar(200),
    consulta_revisao varchar(200)
);